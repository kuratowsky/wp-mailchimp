msgid "Name"
msgstr "Nom"

#: mailchimp.php:581
msgid "Tag"
msgstr "Etiqueta"

#: mailchimp.php:582
msgid "Required?"
msgstr "Obligatori?"

#: mailchimp.php:583
msgid "Include?"
msgstr "Incloure?"

